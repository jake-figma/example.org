{
  "tag": "Button",
  "children": "@label",
  "props": {
    "ariaLabel?": "\"Only when label is not descriptive of the action\"",
    "component": {
      "button": {
        "type": {
          "button": {
            "onClick": "() => alert(\"Required click event\")"
          }
        }
      }
    },
    "disabled?": "true",
    "iconEnd?": "<@iconEnd />",
    "iconStart?": "<@iconStart />",
    "variant": "\"@variant\""
  },
  "permutations": {
    "component": {
      "a": {
        "href": "\"https://www.example.org\"",
        "onClick?": "() => alert(\"Optional click event\")"
      },
      "button": {
        "type": {
          "button": {
            "onClick": "() => alert(\"Required click event\")"
          },
          "submit": {
            "onClick?": "() => alert(\"Optional click event\")"
          }
        }
      }
    }
  }
}
