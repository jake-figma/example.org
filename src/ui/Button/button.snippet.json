{
  "name": "Button",
  "alternates": ["ButtonTwo"],
  "params": {
    "@iconEnd": "undefined",
    "@iconStart": "undefined",
    "@label": "Hello world!",
    "@state": "undefined",
    "@variant": "primary"
  },
  "props": {
    "*": "string",
    "disabled": "boolean",
    "iconEnd": "node",
    "iconStart": "node",
    "onClick": "function"
  },
  "ButtonTwo": "Button",
  "Button": {
    "Required": {
      "children": "@label",
      "disabled": "@state=disabled?true:undefined",
      "iconEnd": "<@iconEnd />",
      "iconStart": "<@iconStart />",
      "onClick": "() => {}",
      "variant": "@variant"
    },
    "All": {
      "ariaLabel": "Only when label is not descriptive of the action",
      "children": "@label",
      "component": "button",
      "disabled": "@state=disabled?true:undefined",
      "iconEnd": "<@iconEnd />",
      "iconStart": "<@iconStart />",
      "onClick": "() => {}",
      "type": "button",
      "variant": "@variant"
    },
    "Component/Type Permutations": [
      { "component": "button", "type": "button", "onClick": "() => {}" },
      { "component": "button", "type": "submit" },
      { "component": "a", "href": "https://www.example.org" }
    ]
  }
}
