{
  "name": "IconButton",
  "params": {
    "@icon": "undefined",
    "@state": "undefined",
    "@variant": "primary"
  },
  "props": {
    "*": "string",
    "disabled": "boolean",
    "icon": "node",
    "onClick": "function"
  },
  "IconButton": {
    "props": {
      "Required": {
        "ariaLabel": "Describe the action",
        "disabled": "@state=disabled?true:undefined",
        "icon": "<@icon />",
        "onClick": "() => {}",
        "variant": "@variant"
      },
      "All": {
        "ariaLabel": "Describe the action",
        "component": "button",
        "disabled": "@state=disabled?true:undefined",
        "icon": "<@icon />",
        "onClick": "() => {}",
        "type": "button",
        "variant": "@variant"
      },
      "Component/Type Permutations": [
        { "component": "button", "type": "button", "onClick": "() => {}" },
        { "component": "button", "type": "submit" },
        { "component": "a", "href": "https://www.example.org" }
      ]
    }
  }
}
